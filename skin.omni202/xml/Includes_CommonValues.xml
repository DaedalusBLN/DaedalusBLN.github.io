<?xml version="1.0" encoding="UTF-8"?>
<includes>
    <include name="FullscreenDimensions">
        <left>0</left>
        <top>0</top>
        <width>FullScreenWidth</width>
        <height>1080</height>
    </include>
    <include name="HiddenObject">
        <left>-300</left>
        <top>-300</top>
        <width>1</width>
        <height>1</height>
        <texturefocus />
        <texturenofocus />
        <label />
        <font />
    </include>
    <include name="ButtonCommonValues">
        <width>630</width>
        <height>90</height>
        <font>font14</font>
        <textoffsetx>30</textoffsetx>
    </include>
    <include name="ButtonInfoDialogsCommonValues">
        <width>277</width>
        <align>center</align>
        <font>font13_title</font>
        <textoffsetx>10</textoffsetx>
        <texturenofocus colordiffuse="33787878">selecteditem_bg.png</texturenofocus>
    </include>
    <!-- KeyBoard -->
    <include name="Objects_KeyOffVars">
        <textoffsety>32</textoffsety>
        <align>center</align>
        <font>font15</font>
        <selectedcolor>highlight</selectedcolor>
        <texturenofocus colordiffuse="11ffffff">selecteditem_bg.png</texturenofocus>
        <alttexturenofocus colordiffuse="33ffffff">selecteditem_bg.png</alttexturenofocus>
    </include>
    <include name="Objects_DoubleKeyOff">
        <left>-123</left>
        <width>290</width>
        <height>70</height>
        <include>Objects_KeyOffVars</include>
    </include>
    <include name="Objects_SingleKeyOff">
        <width>70</width>
        <height>70</height>
        <include>Objects_KeyOffVars</include>
    </include>
    <include name="SettingsPanelButtonsValues">
        <height>90</height>
        <width>1256</width>
        <textoffsetx>15</textoffsetx>
        <textoffsety>-4</textoffsety>
        <aligny>top</aligny>
        <font>font15</font>
    </include>
    <!-- Media Flags -->
    <include name="MediaFlagVars">
        <width>135</width>
        <height>90</height>
        <aspectratio>keep</aspectratio>
        <fadetime>250</fadetime>
        <colordiffuse>maintext</colordiffuse>
        <animation effect="fade" time="200">VisibleChange</animation>
    </include>  
    <include name="SkinSettingsButtonDefaults">
        <width>1246</width>
        <height>90</height>
        <textoffsetx>30</textoffsetx>
        <font>font15</font>
    </include>
    <include name="AutoscrollDefault">
        <autoscroll time="3000" delay="4000" repeat="5000">!Skin.HasSetting(Disable.AutoScroll)</autoscroll>
    </include>
