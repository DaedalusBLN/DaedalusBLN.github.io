<?xml version="1.0" encoding="UTF-8"?>
<window>
    <defaultcontrol always="true">450</defaultcontrol>
    <allowoverlay>no</allowoverlay>
    <animation effect="slide" start="0,878" end="0,0" time="300" tween="quadratic" easing="out">WindowOpen</animation>
    <animation effect="slide" start="0,0" end="0,878" time="300" tween="quadratic" easing="out">WindowClose</animation>
    <coordinates>
        <system>1</system>
        <left>0</left>
        <top>202</top>
    </coordinates>
    <controls>
        <control type="image">
            <width>1920</width>
            <height>878</height>
            <texture colordiffuse="background">$VAR[CommonBackgroundOverlay]</texture>
        </control>
        <control type="image">
            <top>8</top>
            <width>1920</width>
            <height>870</height>
            <texture colordiffuse="background">dialog.png</texture>
        </control>
        <control type="label" id="2">
            <include>HiddenObject</include>
        </control>
        <control type="group">
            <top>48</top>
            <left>72.5</left>
            <control type="panel" id="450">
                <left>0</left>
                <top>0</top>
                <width>1775</width>
                <height>790</height>
                <onup>450</onup>
                <ondown>450</ondown>
                <onleft>450</onleft>
                <onright condition="Container(450).HasNext | Container(450).HasPrevious">60</onright>
                <onright condition="!Container(450).HasNext + !Container(450).HasPrevious">450</onright>
                <preloaditems>2</preloaditems>
                <pagecontrol>60</pagecontrol>
                <viewtype label="21371">list</viewtype>
                <scrolltime tween="quadratic" easing="out">300</scrolltime>
                <orientation>vertical</orientation>
                <itemlayout height="395" width="355">
                    <control type="image">
                        <top>20</top>
                        <left>20</left>
                        <width>315</width>
                        <height>315</height>
                        <texture>overlays/diffuse-overlay.png</texture>
                    </control>
                    <control type="label">
                        <left>20</left>
                        <right>20</right>
                        <top>333</top>
                        <height>50</height>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <control type="image">
                        <left>20</left>
                        <top>20</top>
                        <width>315</width>
                        <height>315</height>
                        <aspectratio align="center" aligny="center">keep</aspectratio>
                        <texture background="true" fallback="DefaultFolder.png">$VAR[ListIcon]</texture>
                    </control>
                </itemlayout>
                <focusedlayout height="395" width="355">
                    <include content="SelectedItemBackground">
                        <param name="Left" value="10" />
                        <param name="Top" value="10" />
                        <param name="Width" value="335" />
                        <param name="Height" value="375" />
                    </include>
                    <control type="image">
                        <top>20</top>
                        <left>20</left>
                        <width>315</width>
                        <height>315</height>
                        <texture>overlays/diffuse-overlay.png</texture>
                    </control>
                    <control type="label">
                        <left>20</left>
                        <right>20</right>
                        <top>333</top>
                        <height>50</height>
                        <label>$INFO[ListItem.Label]</label>
                    </control>
                    <control type="image">
                        <left>20</left>
                        <top>20</top>
                        <width>315</width>
                        <height>315</height>
                        <aspectratio align="center" aligny="center">keep</aspectratio>
                        <texture background="true" fallback="DefaultFolder.png">$VAR[ListIcon]</texture>
                    </control>
                </focusedlayout>
            </control>
        </control>
        <control type="scrollbar" id="60">
            <visible allowhiddenfocus="true">Control.HasFocus(60) | Container(450).Scrolling</visible>
            <animation effect="fade" time="300" tween="sine" easing="inout" start="0" end="100">Visible</animation>
            <left>1850</left>
            <top>68</top>
            <width>10</width>
            <height>710</height>
            <onup>60</onup>
            <ondown>60</ondown>
            <onleft>450</onleft>
            <onright>450</onright>
            <orientation>vertical</orientation>
        </control>
    </controls>
</window>